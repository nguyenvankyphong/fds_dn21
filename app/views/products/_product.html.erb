<div class="col-lg-3 col-md-4 col-sm-6 foo" data-sr-id="1" style="; visibility: visible;  -webkit-transform: translateY(0) scale(1); opacity: 1;transform: translateY(0) scale(1); opacity: 1;-webkit-transition: -webkit-transform 0.7s cubic-bezier(0.6, 0.2, 0.1, 1) 0s, opacity 0.7s cubic-bezier(0.6, 0.2, 0.1, 1) 0s; transition: transform 0.7s cubic-bezier(0.6, 0.2, 0.1, 1) 0s, opacity 0.7s cubic-bezier(0.6, 0.2, 0.1, 1) 0s; ">
   <div class="popular__food">
      <div class="pp_food__thumb">
         <%= link_to product_path(product) do %>
           <%= image_tag product.picture.url %>
         <% end %>
         <div class="pp__food__prize">
            <span><%= product.price %></span>
         </div>
      </div>
      <div class="pp__food__inner">
         <div class="pp__fd__icon">
            <%= image_tag product.picture.url %>
         </div>
         <div class="pp__food__details">
            <h4><a href="menu-details.html"><%= link_to product.name, product_path(product) %></a></h4>
            <%= render "products/rate", product: product%>
         </div>
      </div>
   </div>
</div>
